	<!--품질평가 list count-->
	<select id="selectQualityEvaluationPeriodicListCount" parameterClass="SRMEVL0060VO" resultClass="Integer">
		/*SRMEVL0060.selectQualityEvaluationPeriodicListCount*/
		SELECT
				COUNT(*)
		FROM
				SSUGL_PROCESS_SITEVISIT A
				,SEVUS D
				,SSUGL_PARTNER B
				,SSGMT C
		WHERE
				A.HOUSE_CODE = D.HOUSE_CODE
			AND A.EVAL_NO_RESULT = D.EV_NO
			AND A.HOUSE_CODE = B.HOUSE_CODE
			AND A.SELLER_CODE = B.SELLER_CODE
			AND B.HOUSE_CODE = C.HOUSE_CODE
			AND NVL(B.ZZMCHL1,'999') = C.SG_NO(+)
			AND A.EVAL_FLAG = '1'
			AND A.EVAL_SELLER_CODE = #evalSellerCode#
			<!--AND D.EV_USER_ID = #evUserId#-->
			<isNotEmpty property="sellerNameLoc">
				AND SELLER_NAME_LOC LIKE '%'||#sellerNameLoc#||'%'
			</isNotEmpty>
			<isNotEmpty property="status">
				<!--<isEqual property="status" compareValue="100">-->
					<!--AND A.STATUS IN ('G04', 'G05')-->
				<!--</isEqual>-->
				<!--<isEqual property="status" compareValue="200">-->
					<!--AND A.STATUS = 'G06'-->
				<!--</isEqual>-->
				<!--<isEqual property="status" compareValue="300">-->
					<!--AND A.STATUS = 'G08'-->
				<!--</isEqual>-->
				AND D.PROGRESS_CODE = #status#
			</isNotEmpty>

	</select>

	<!--품질평가 list-->
	<select id="selectQualityEvaluationPeriodicList" parameterClass="SRMEVL0060VO" resultMap="selectQualityEvaluationPeriodicListMap">
		/*SRMEVL0060.selectQualityEvaluationPeriodicList*/
		SELECT 	*
		FROM 	(
				SELECT 	ROWNUM RNUM
						, A.*
				FROM 	(
						SELECT
								A.HOUSE_CODE
								,A.SELLER_CODE
								,A.SEQ
								,A.VISIT_SEQ
								,EVAL_SELLER_CODE
								,EVAL_NO_RESULT
								,A.SELLER_NAME_LOC
								,C.SG_NO AS CAT_LV1_CODE
								,C.SG_NAME AS CAT_LV1_CODE_NAME
								,TO_CHAR(A.REQ_DATE,'YYYY-MM-DD') AS REQ_DATE
								,TO_CHAR(A.RECEIPT_DATE,'YYYY-MM-DD') AS RECEIPT_DATE
								,A.CHANGE_DATE
								,A.STATUS
								,D.PROGRESS_CODE
								,GETCODETEXT1('SRM900', A.STATUS, UPPER(#locale#)) AS STATUS_NM
								,EV_TPL_NO
						FROM
								SSUGL_PROCESS_SITEVISIT A
								,SEVUS D
								,SSUGL_PARTNER B
								,SSGMT C
						WHERE
								A.HOUSE_CODE = D.HOUSE_CODE
							AND A.EVAL_NO_RESULT = D.EV_NO
							AND A.HOUSE_CODE = B.HOUSE_CODE
							AND A.SELLER_CODE = B.SELLER_CODE
							AND B.HOUSE_CODE = C.HOUSE_CODE
							AND NVL(B.ZZMCHL1,'999') = C.SG_NO(+)
							AND A.EVAL_FLAG = '1'
							AND A.EVAL_SELLER_CODE = #evalSellerCode#
							<!--AND D.EV_USER_ID = #evUserId#-->
							<isNotEmpty property="sellerNameLoc">
								AND SELLER_NAME_LOC LIKE '%'||#sellerNameLoc#||'%'
							</isNotEmpty>
							<isNotEmpty property="status">
								<!--<isEqual property="status" compareValue="100">-->
									<!--AND A.STATUS IN ('G04', 'G05')-->
								<!--</isEqual>-->
								<!--<isEqual property="status" compareValue="200">-->
									<!--AND A.STATUS = 'G06'-->
								<!--</isEqual>-->
								<!--<isEqual property="status" compareValue="300">-->
									<!--AND A.STATUS = 'G08'-->
								<!--</isEqual>-->
								AND D.PROGRESS_CODE = #status#
							</isNotEmpty>
							ORDER BY EVAL_NO_RESULT DESC, SEQ DESC
						)A
				)
		WHERE RNUM BETWEEN #firstIndex# AND #lastIndex#
	</select>


	<!--품질평가 list Excel-->
	<select id="selectQualityEvaluationPeriodicListExcel" parameterClass="SRMEVL0060VO" resultMap="selectQualityEvaluationPeriodicListMap">
		/*SRMEVL0060.selectQualityEvaluationPeriodicListExcel*/
		SELECT 	*
		FROM 	(
					SELECT 	ROWNUM RNUM
							, A.*
					FROM 	(
								SELECT
										A.HOUSE_CODE
										,A.SELLER_CODE
										,A.SEQ
										,A.VISIT_SEQ
										,EVAL_SELLER_CODE
										,EVAL_NO_RESULT
										,A.SELLER_NAME_LOC
										,C.SG_NO AS CAT_LV1_CODE
										,C.SG_NAME AS CAT_LV1_CODE_NAME
										,TO_CHAR(A.REQ_DATE,'YYYY-MM-DD') AS REQ_DATE
										,TO_CHAR(A.RECEIPT_DATE,'YYYY-MM-DD') AS RECEIPT_DATE
										,A.CHANGE_DATE
										,A.STATUS
										,D.PROGRESS_CODE
										,CASE WHEN D.PROGRESS_CODE='300' THEN GETCODETEXT1('SRM900', 'G08', UPPER(#locale#)) ELSE GETCODETEXT1('SRM900', A.STATUS, UPPER(#locale#))END AS STATUS_NM
										,EV_TPL_NO
								FROM
										SSUGL_PROCESS_SITEVISIT A
										,SEVUS D
										,SSUGL_PARTNER B
										,SSGMT C
								WHERE
										A.HOUSE_CODE = D.HOUSE_CODE
									AND A.EVAL_NO_RESULT = D.EV_NO
									AND A.HOUSE_CODE = B.HOUSE_CODE
									AND A.SELLER_CODE = B.SELLER_CODE
									AND B.HOUSE_CODE = C.HOUSE_CODE
									AND NVL(B.ZZMCHL1,'999') = C.SG_NO(+)
									AND A.EVAL_FLAG = '1'
									AND A.EVAL_SELLER_CODE = #evalSellerCode#
									<!--AND D.EV_USER_ID = #evUserId#-->
									<isNotEmpty property="sellerNameLoc">
										AND SELLER_NAME_LOC LIKE '%'||#sellerNameLoc#||'%'
									</isNotEmpty>
									<isNotEmpty property="status">
										<!--<isEqual property="status" compareValue="100">-->
											<!--AND A.STATUS IN ('G04', 'G05')-->
										<!--</isEqual>-->
										<!--<isEqual property="status" compareValue="200">-->
											<!--AND A.STATUS = 'G06'-->
										<!--</isEqual>-->
										<!--<isEqual property="status" compareValue="300">-->
											<!--AND A.STATUS = 'G08'-->
										<!--</isEqual>-->
										AND D.PROGRESS_CODE = #status#
									</isNotEmpty>
								ORDER BY EVAL_NO_RESULT DESC, SEQ DESC
							)A
				)
	</select>

	<!--파트너사 정보-->
	<select id="selectQualityEvaluationCompInfo" parameterClass="SRMEVL0060VO" resultMap="selectQualityEvaluationCompInfoMap">
		/*SRMEVL0060.selectQualityEvaluationCompInfo*/
		SELECT
				A.HOUSE_CODE
				, A.SELLER_CODE
				, A.SEQ
				<!--, GETCODETEXT1('SRM053',B.CHANNEL_CODE,UPPER('ko')) AS CHANNEL_CODE_NM-->
				, GETSGCLASS('000', UPPER('ko'), '1', b.ZZMCHL1) AS CAT_LV1_CODE_NAME
				<!--, B.SHIPPER_TYPE-->
				<!--, GETCODETEXT1('M076',B.SHIPPER_TYPE,UPPER('ko')) AS SHIPPER_TYPE_NAME-->
				, B.IRS_NO
				, A.SELLER_NAME_LOC
				, GETCODETEXT1('M074',B.COMPANY_TYPE, UPPER('ko')) AS COMPANY_TYPE_NM
				, B.SELLER_CEO_NAME
				, A.V_MAIN_NAME
				, B.V_PHONE1
				, A.V_MOBILE_PHONE
				, A.V_EMAIL
				, B.FAX_NO
				, A.SELLER_ADDRESS
				, B.INDUSTRY_TYPE
				, B.BUSINESS_TYPE
				, GETCODETEXT1('M076',B.SELLER_TYPE, UPPER('ko')) AS SELLER_TYPE_NM
				, GETCODETEXT1('M001',B.COUNTRY, UPPER('ko')) AS COUNTRY_NM
				, B.REGIO
				<!--, B.FOUNDATION_DATE-->
				, B.BASIC_AMT
				, B.ZREVN
				<!--, B.EMP_COUNT-->
				<!--, B.PLANT_OWN_TYPE-->
				<!--, B.PLANT_ROLE_TYPE-->
				<!--, GETCODETEXT1('M729',B.PLANT_ROLE_TYPE, UPPER('ko')) AS PLANT_ROLE_TYPE_NM-->
				<!--, B.MAIN_CUSTOMER-->
				<!--, B.ABOARD_CHANNEL_TEXT-->
				<!--, B.ABOARD_COUNTRY_TEXT-->
				, A.REMARK
				, A.STATUS
				, GETCODETEXT1('SRM900',A.STATUS,UPPER('ko')) AS STATUS_NM
				, D.PROGRESS_CODE
				, TO_CHAR(A.REQ_DATE, 'YYYY-MM-DD') AS REQ_DATE
				, TO_CHAR(A.RECEIPT_DATE, 'YYYY-MM-DD') AS RECEIPT_DATE
				, TO_CHAR(TO_DATE(A.CHECK_DATE, 'YYYY-MM-DD'),'YYYY-MM-DD') AS CHECK_DATE
				, A.EVAL_NO_RESULT
				, A.CHECK_TIME
				, B.ZZQC_FG1
				, B.ZZQC_FG2
				, B.ZZQC_FG3
				, B.ZZQC_FG4
				, B.ZZQC_FG5
				, B.ZZQC_FG6
				, B.ZZQC_FG7
				, B.ZZQC_FG8
				, B.ZZQC_FG9
				, B.ZZQC_FG10
				, B.ZZQC_FG11
				, B.ZZQC_FG12
				, A.EVAL_FLAG
		FROM
				SSUGL_PROCESS_SITEVISIT A
				, SSUGL_PARTNER B
				, SEVUS D
		WHERE
				A.HOUSE_CODE = B.HOUSE_CODE
			AND A.SELLER_CODE = B.SELLER_CODE
			AND A.HOUSE_CODE = D.HOUSE_CODE
			AND A.EVAL_NO_RESULT = D.EV_NO
			AND A.HOUSE_CODE = #houseCode#
			AND A.SELLER_CODE = #sellerCode#
			AND A.SEQ = #seq#
			AND A.VISIT_SEQ = #visitSeq#
	</select>
</sqlMap>

